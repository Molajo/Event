{
    "name": "molajo/event",
    "type": "molajo-package",
    "description": "Molajo Events: Dispatcher, Event Dispatcher and Event Package for PHP applications",
    "keywords": ["Event", "Molajo", "CommonApi"],
    "homepage": "http://github.com/Molajo/Event",
    "license": "MIT",
    "authors": [
        {
            "name": "Amy Stephen",
            "email": "AmyStephen@Molajo.org",
            "homepage": "http://AmyStephen.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "commonapi/event": "dev-master",
        "commonapi/exception": "dev-master"
    },
    "support": {
        "event": "hello@molajo.org",
        "irc": "irc://irc.freenode.org/molajo"
    },
    "autoload": {
        "psr-4": {
            "Molajo\\Event\\": "Source/",
            "Molajo\\Plugins\\": "Plugins/",
            "Molajo\\Factories\\Dispatcher\\": "Factories/Dispatcher/",
            "Molajo\\Factories\\Event\\": "Factories/Event/"
        }
    },

    "archive": {
        "exclude": ["/.dev"]
    },

    "minimum-stability": "dev"
}
